program   sdsd    ;
var result,initial,final : integer ;
procedure func1;
begin
    write("Start value");
    read(initial);
    func1;
    write("End value");
    read(final);

    result := initial;

    while result < final do
        begin
            result := result + 1;
            write(result);
        end;

    for(  ; result > initial ; result := result - 1)
        begin
            write(result);
        end;
end.